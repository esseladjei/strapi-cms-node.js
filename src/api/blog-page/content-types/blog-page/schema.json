{
  "kind": "singleType",
  "collectionName": "blog_pages",
  "info": {
    "singularName": "blog-page",
    "pluralName": "blog-pages",
    "displayName": "BlogPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "Our Blog",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "pageInfo": {
      "type": "component",
      "repeatable": true,
      "component": "layout.page-info"
    },
    "excludedtags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "dyn": {
      "type": "dynamiczone",
      "components": [
        "blog.post-selection",
        "layout.feature-course",
        "config.social-link"
      ]
    }
  }
}
